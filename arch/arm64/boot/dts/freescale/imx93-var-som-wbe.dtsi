#include "imx93-var-som.dtsi"

&ads7846_ts {
	status = "disabled";
};

&lpspi8 {
	/* 802.15.4 SPI device */
	spidev@0 {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_restouch>;
		reg = <0>;
		compatible = "var,spidev";
		spi-max-frequency = <1000000>;
		status = "okay";
	};
};
