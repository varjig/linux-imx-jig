&mipi_dsi_bridge {
	status = "disabled";

	panel@0 {
		reg = <0>;
		status = "okay";
		compatible = "sgd,gktw70sdae4sd";
		dsi-lanes = <4>;
		panel-width-mm  = <154>;
		panel-height-mm = <87>;

		port {
			panel_in: endpoint {
				remote-endpoint = <&mipi_dsi_bridge_out>;
			};
		};
	};

	port@1 {
		mipi_dsi_bridge_out: endpoint {
			remote-endpoint = <&panel_in>;
		};
