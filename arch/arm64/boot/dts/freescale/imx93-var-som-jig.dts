/dts-v1/;

#include "imx93-var-som.dtsi"

/ {
	model = "Variscite VAR-SOM-MX93 on JIG Adapter";

	aliases {
		gpio4 = &pca9534;
		ethernet0 = &eqos;
	};

	chosen {
		stdout-path = &lpuart1;
	};

	cm33: imx93-cm33 {
		status="disable";
	};

	ethosu {
		status="disable";
	};

#if 0
