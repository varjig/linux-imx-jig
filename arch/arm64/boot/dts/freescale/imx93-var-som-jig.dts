	ov5640: camera@3c {
		compatible = "ovti,ov5640";
		reg = <0x3c>;
		status = "disabled";

		clocks = <&clk_ov5640_fixed>;
		clock-names = "xclk";

		csi_id = <0>;
		mclk = <24000000>;
		mclk_source = <0>;
		mipi_csi;
	};
	
