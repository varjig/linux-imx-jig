/* Console */
&lpuart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&mu1 {
	status = "okay";
};

&mu2 {
	status = "okay";
};

&usbotg1 {
	dr_mode = "peripheral";
	status = "okay";
};

&usbotg2 {
	disable-over-current;
	dr_mode = "host";
	status = "okay";
};

/* SD */
&usdhc2 {
	pinctrl-names = "default", "state_100mhz", "state_200mhz";
	pinctrl-0 = <&pinctrl_usdhc2>;
	pinctrl-1 = <&pinctrl_usdhc2>;
	pinctrl-2 = <&pinctrl_usdhc2>;
	
	vmmc-supply = <&reg_usdhc2_vmmc>;
	broken-cd;
	bus-width = <4>;
	status = "okay";
	no-mmc;
	ni-sdio;
};

&ele_mu {
	status = "okay";
};

&lpspi8 {
	cs-gpios = <0>;
	num-cs = <1>;
};
